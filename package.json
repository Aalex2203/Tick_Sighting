{
  "name": "tick-tracker",
  "version": "1.0.0",
  "private": true,
  "description": "A full-stack tick tracking application",
  "author": "Your Name",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/tick-tracker.git"
  },
  "keywords": [
    "full-stack",
    "javascript",
    "nodejs",
    "express"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npx live-server --port=3000",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npx live-server --port=3000",
    "kill": "taskkill /F /IM node.exe"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "concurrently": "^8.2.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.21",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.13.5",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "multer": "^2.0.2",
    "react-hook-form": "^7.71.1",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  }
}
